library("pheatmap")
library("RColorBrewer")
a=read.table("C:\\Users\\东东\\Desktop\\发酵过程中共有的鲜味肽.csv",head=T,sep=",")
rownames(a)=a[,1]
a=a[,-1]
x=data.matrix(a)
coul <- colorRampPalette(brewer.pal(10, "RdBu")[10:1])(50)
pheatmap(x,scale="row",col=coul,cutree_rows = 3,cutree_cols = 2,
show_rownames=T,show_colnames=T,fontsize_col = 10,fontsize_row = 10,angle_col=315,
border_color= "white",cellwidth=NA,
cluster_rows=T,scale_cols=FALSE,scale_row=FALSE,cluster_cols=T)


